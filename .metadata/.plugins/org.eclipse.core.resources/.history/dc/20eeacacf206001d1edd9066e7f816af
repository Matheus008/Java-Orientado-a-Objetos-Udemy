package exercícios;

import java.util.Scanner;

public class exe001 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int num01 = sc.nextInt(10);
		int num02 = sc.nextInt(30);
		int som01 = num01 + num02;
		
		int num03 = sc.nextInt(-30);
		int num04 = sc.nextInt(10);
		int som02 = num03 + num04;
		
		int num05 = sc.nextInt(0);
		int num06 = sc.nextInt(0);
		int som03 = num05 + num06;
		
		System.out.println("Soma dos números 1 e 2: "+ som01);
		
		sc.close();
	}

}
